{
  "name": "typescript-react-redux-starter",
  "version": "0.1.0",
  "description": "A boiler-plate setup for TypeScript/React/Redux applications",
  "license": "MIT",
  "engines": {
    "node": "4.x"
  },
  "scripts": {
    "postinstall": "npm run clean && npm run typings && npm run build",
    "dev": "cross-env NODE_ENV=development webpack-dev-server -d --inline --hot --progress --no-info",
    "start": "cross-env NODE_ENV=production node server/node-server.js",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack -p",
    "typings": "rimraf typings && typings install",
    "test": "npm run lint && ts-node ./node_modules/mocha/bin/_mocha --opts ./test/mocha.opts",
    "test:watch": "ts-node ./node_modules/mocha/bin/_mocha --opts ./test/mocha.opts --watch",
    "lint": "tslint 'src/**/*.tsx' 'src/**/*.ts' 'test/**/*.tsx' 'test/**/*.ts'"
  },
  "keywords": [
    "react",
    "redux",
    "typescript",
    "starter",
    "seed"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rangle/typescript-react-redux-starter.git"
  },
  "bugs": {
    "url": "https://github.com/rangle/rangle-starter/issues"
  },
  "dependencies": {
    "express": "^4.13.4",
    "helmet": "^3.1.0",
    "http-proxy": "^1.13.2",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "basscss": "^8.0.1",
    "basscss-addons": "^1.0.0-beta4",
    "basscss-basic": "^1.0.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.0",
    "es5-shim": "^4.5.6",
    "es6-promise": "^4.0.5",
    "es6-shim": "^0.35.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "history": "^4.4.0",
    "html-webpack-plugin": "^2.9.0",
    "immutable": "^3.7.6",
    "istanbul": "^1.0.0-alpha.2",
    "jsdom": "^9.8.3",
    "mocha": "^3.1.2",
    "object-assign": "^4.0.1",
    "postcss-cssnext": "^2.4.0",
    "postcss-import": "^8.0.2",
    "postcss-loader": "^1.1.1",
    "radium": "^0.18.1",
    "raw-loader": "^0.5.1",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-redux": "^4.0.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.0.4",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "redux-form": "^6.2.0",
    "redux-localstorage": "^0.4.0",
    "redux-logger": "^2.1.4",
    "redux-thunk": "^2.0.1",
    "require-hacker": "^2.1.3",
    "rimraf": "^2.4.3",
    "style-loader": "^0.13.0",
    "ts-loader": "^1.2.2",
    "ts-node": "^1.7.0",
    "tslint": "^4.0.1",
    "tslint-loader": "^2.1.0",
    "typescript": "^2.0.10",
    "typings": "^2.0.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.9.1"
  }
}
